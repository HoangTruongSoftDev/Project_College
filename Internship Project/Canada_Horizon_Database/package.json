{
  "name": "package-complete",
  "productName": "Nouvel Horizon Canada",
  "version": "1.0.0",
  "description": "Canada Horizon Database Management",
  "main": "main.js",
  "scripts": {
    "start": "electron main.js",
    "package": "electron-builder build"
  },
  "author": "Hoang Truong Pham",
  "license": "ISC",
  "devDependencies": {
    "electron": "^30.0.2",
    "electron-builder": "^24.13.3",
    "nodemon": "^3.1.0"
  },
  "build": {
    "appId": "com.Truong.CanadaHorizonDatabase",
    "files": [
      "public/**/*",
      "node_modules/**/*",
      "./package.json",
      "main.js",
      "server/**/*"
    ],
    "extraFiles": [
      "credentials"
    ]
  },
  "dependencies": {
    "axios": "^1.6.8",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "ejs": "^3.1.10",
    "express": "^4.19.2",
    "express-fileupload": "^1.5.0",
    "gridfs-stream": "^1.1.1",
    "method-override": "^3.0.0",
    "mongodb": "^6.6.1",
    "mongoose": "^8.4.0",
    "multer-gridfs-storage": "^1.3.0",
    "randomstring": "^1.3.0",
    "webpack": "^5.91.0"
  }
}